import math

def calculate_area(shape, size):
    if shape == 'S':
        area = size ** 2
        print(f"Area of Square = {area}")
    elif shape == 'C':
        area = math.pi * (size ** 2)
        print(f"Area of Circle = {area:.2f}")
    else:
        print("Invalid shape input")

# Sample Input 1
calculate_area('S', 4)

# Sample Input 2
calculate_area('C', 4)
