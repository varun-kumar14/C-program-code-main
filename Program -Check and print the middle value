#include <stdio.h>

int main() {
    int x, y, z;

    // Input
    printf("Enter three distinct values: ");
    scanf("%d %d %d", &x, &y, &z);

    // Find the middle value
    if ((x > y && x < z) || (x < y && x > z))
        printf("1\n");
    else if ((y > x && y < z) || (y < x && y > z))
        printf("2\n");
    else
        printf("3\n");

    return 0;
}
